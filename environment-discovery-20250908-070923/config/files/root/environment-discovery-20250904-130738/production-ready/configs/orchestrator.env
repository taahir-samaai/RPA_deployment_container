# Orchestrator Configuration - t3.medium optimized
ORCHESTRATOR_HOST=0.0.0.0
ORCHESTRATOR_PORT=8620
WORKER_ENDPOINTS=["http://worker1:8621/execute","http://worker2:8621/execute"]

# Threading configuration for t3.medium (2 vCPU, 4GB RAM)
MAX_WORKERS=3
WORKER_TIMEOUT=600
JOB_POLL_INTERVAL=20
BATCH_SIZE=2

# Database and storage
BASE_DATA_DIR=/app/data
DB_POOL_SIZE=5

# Security - Generate these properly in production
JWT_SECRET=your-secret-jwt-key-change-this-in-production
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Logging
LOG_LEVEL=INFO
